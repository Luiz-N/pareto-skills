            <h3 class="panel-title">Panel title</h3>
          </div> --}}
          <div class="short-chart panel-body col-md-6">
            {{#if shortHeight}}
              {{dc-dashboard/ordinal-boxplot
                dimension=projectDim
                groups=projectGroupsBarChart
                width=shortWidth
                height=shortHeight
                colors=projectColors
                order=projectOrder
                id="projectBoxPlot"
              }}
            {{/if}}
          </div>

          <div class="short-chart panel-body col-md-6">
            {{#if shortHeight}}
              {{dc-dashboard/ordinal-boxplot
                dimension=unitDim
